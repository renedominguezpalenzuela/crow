<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}
				Welcome!
			{% endblock %}
		</title>


		{{ encore_entry_link_tags('css/global') }}
		<link rel="stylesheet" href="{{ asset('librerias/css/material-dashboard.css?v=2.1.0') }}">
		<link rel="stylesheet" href="{{ asset('librerias/css/myestilos.css') }}"> {% block css %}{% endblock %}
		{# 
			<link rel="stylesheet" href="{{ asset('https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons') }}">
			<link rel="stylesheet"  href="{{ asset('https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css') }}">
		#}

		{% block stylesheets %}
		{% endblock %}
	</head>

	<body id="main_body" class="container-fluid"> 
	
		
		{% block body %}
		
		
		{% endblock %}


		

		<script src="{{ asset('librerias/js/jquery.min.js') }}" type="text/javascript"></script>
		<script src="{{ asset('librerias/js/popper.min.js') }}" type="text/javascript"></script>

		{{ encore_entry_script_tags('js/app') }}
		{#<script src="{{ asset('librerias/js/core/bootstrap-material-design.min.js') }}" type="text/javascript"></script>#}
		{#<script src="{{ asset('librerias/js/plugins/perfect-scrollbar.jquery.min.js') }}" type="text/javascript"></script>#}
		<script src="{{ asset('librerias/js/plugins/bootstrap-notify.js') }}" type="text/javascript"></script>
		{#<script src="{{ asset('librerias/js/material-dashboard.js') }}" type="text/javascript"></script>#}


	<script>
			$(document).ready(function () {
					{% for tipo, mensajes in app.session.flashbag.all() %}
					{% for mensaje in mensajes %}
							$.notify({
							message: '{{ mensaje }}'
							}, {
							type: '{{ tipo }}',
							timer: 3000,
							placement: {
								from: 'top',
								align: 'right'
							}
							});
						{% endfor %}
					{% endfor %}

					var url = window.location;
					var element = $('li a').filter(function () {
						return this.href == url || url.href.indexOf(this.href) == 0;
					}).addClass('active').parent();

					if (element.is('li')) {
						element.parent().parent().addClass('active');
						element.parent().addClass('in');
						element.addClass('active');
						element.children().removeClass('active');
					}
				});




		</script>

		{% block javascripts %}

		
		{% endblock %}

		
	

	</body>
</html>
